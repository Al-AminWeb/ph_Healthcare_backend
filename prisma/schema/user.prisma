  // const user = { password: 'plaintext123' };  // âŒ Security disaster!
  //
  // ALWAYS DO THIS:
  // const hashedPassword = await bcrypt.hash('plaintext123', 10);
  // const user = { password: hashedPassword };  // âœ… Safe
  //
