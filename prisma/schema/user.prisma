  // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
  // FOREIGN KEY: Links to User
  // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
  //
  // userId: The User this profile belongs to
  // @unique â†’ One admin profile per user (enforces 1:1)
  //
  // user: The relation to User model
  // @relation(fields: [userId], references: [id], onDelete: Cascade)
  //
  // RELATION BREAKDOWN:
  // - fields: [userId]    â†’ This model's foreign key column
  // - references: [id]    â†’ User model's primary key
  // - onDelete: Cascade   â†’ Delete admin when user deleted
  //
  // CASCADE MEANS:
  // If User is deleted â†’ Admin profile is AUTOMATICALLY deleted
  // This maintains referential integrity (no orphan profiles)
  //
  userId String @unique
  user   User   @relation(fields: [userId], references: [id], onDelete: Cascade)

  // Indexes
  @@index([email], map: "admin_email_idx")
  @@index([userId], map: "admin_user_idx")
  @@map("admins")
}

