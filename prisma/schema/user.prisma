  // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
  // PASSWORD: Hashed password (NEVER store plain text!)
  // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
  // Stored as bcrypt hash, not the actual password!
  //
  // BCRYPT HASH FORMAT:
  // $2b$10$N9qo8uLOickgx2ZMRZoMye.IjqQBrkHx3LMnJnDAz1qnmP5qQvOx2
  // â†‘   â†‘  â†‘
  // |   |  â””â”€ Salt + Hash (always same length)
  // |   â””â”€ Cost factor (higher = slower = more secure)
  // â””â”€ Algorithm version
  //
