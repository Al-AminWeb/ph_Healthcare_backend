//
// PURPOSE:
// Stores patient-specific profile information.
// Linked to health data, medical reports, and appointments.
//
// RELATIONSHIPS:
// Patient 1â”€â”€1 PatientHealthData (optional - created on first health update)
// Patient â”€â”€< MedicalReport (one-to-many - uploaded documents)
// Patient â”€â”€< Appointment (one-to-many)
// Patient â”€â”€< Prescription (one-to-many)
// Patient â”€â”€< Review (one-to-many)
//
/// Patient profile - Healthcare consumer details
model Patient {
  id String @id @default(uuid(7))

  // Denormalized email for query performance
  email String @unique @db.VarChar(255)

  // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
  // PERSONAL INFORMATION
  // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
  // Minimal required info - detailed health data is in PatientHealthData
  //
  name          String  @db.VarChar(100)
  profilePhoto  String? @db.VarChar(500)
  contactNumber String? @db.VarChar(20) // Optional for patients
  address       String? @db.VarChar(500)

  // Audit fields
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  isDeleted Boolean   @default(false)
  deletedAt DateTime?

  // Foreign key - links to User
  userId String @unique
  user   User   @relation(fields: [userId], references: [id], onDelete: Cascade)

  // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
  // RELATIONS
  // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
  //
  // patientHealthData: Detailed health information (optional)
  // Created when patient first updates health info
  // Contains: blood group, allergies, medications, etc.
  //
  // medicalReport: Uploaded medical documents
  // Lab results, X-rays, prescriptions from other doctors
  //
  // appointments: All booked appointments
  //
  // prescriptions: All prescriptions received
  //
  // reviews: Reviews submitted for doctors
  //
  patientHealthData PatientHealthData?
  medicalReport     MedicalReport[]
  appointments      Appointment[]
  prescriptions     Prescription[]
  reviews           Review[]

  // Indexes
  @@index([email], map: "patient_email_idx")
  @@index([name], map: "patient_name_idx")
  @@index([isDeleted], map: "patient_deleted_idx")
  @@index([userId], map: "patient_user_idx")
  @@map("patients")
}

